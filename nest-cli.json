{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/onboarding/src",
  "monorepo": true,
  "root": "apps/onboarding",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/onboarding/tsconfig.app.json"
  },
  "projects": {
    "onboarding": {
      "type": "application",
      "root": "apps/onboarding",
      "entryFile": "main",
      "sourceRoot": "apps/onboarding/src",
      "compilerOptions": {
        "tsConfigPath": "apps/onboarding/tsconfig.app.json"
      }
    },
    "relayer": {
      "type": "application",
      "root": "apps/relayer",
      "entryFile": "main",
      "sourceRoot": "apps/relayer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/relayer/tsconfig.app.json"
      }
    },
    "blockchain": {
      "type": "library",
      "root": "libs/blockchain",
      "entryFile": "index",
      "sourceRoot": "libs/blockchain/src",
      "compilerOptions": {
        "tsConfigPath": "libs/blockchain/tsconfig.lib.json"
      }
    },
    "nest-utils": {
      "type": "library",
      "root": "libs/nest-utils",
      "entryFile": "index",
      "sourceRoot": "libs/nest-utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nest-utils/tsconfig.lib.json"
      }
    },
    "tools": {
      "type": "application",
      "root": "apps/tools",
      "entryFile": "main",
      "sourceRoot": "apps/tools/src",
      "compilerOptions": {
        "tsConfigPath": "apps/tools/tsconfig.app.json"
      }
    },
    "komenci-logger": {
      "type": "library",
      "root": "libs/komenci-logger",
      "entryFile": "index",
      "sourceRoot": "libs/komenci-logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/komenci-logger/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    }
  }
}