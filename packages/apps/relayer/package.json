    "@nestjs/cli": "^7.6.0",
    "fetch-mock": "^9.11.0",
    "ts-jest": "^26.5.5"
  },
  "dependencies": {
    "@nestjs/platform-express": "^7.6.15",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "source-map-support": "^0.5.19"
  }
