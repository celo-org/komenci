{
  "name": "komenci",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": [
    "libs/komencikit",
    "load-testing"
  ],
  "scripts": {
    "build": "nest build",
    "build:komencikit": "yarn --cwd ./libs/komencikit build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "prebuild": "rimraf dist",
    "postinstall": "test -n \"$SKIPPOSTINSTALL\" || yarn run postinstall-actual",
    "postinstall-actual": "yarn run build:komencikit",
    "secrets:decrypt": "bash scripts/secrets.sh decrypt",
    "secrets:encrypt": "bash scripts/secrets.sh encrypt",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:cov": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest.config.e2e.js --runInBand",
    "test:e2e:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config jest.config.e2e.js --runInBand",
    "tools": "node dist/apps/tools/main.js",
    "tools:dev": "nest start tools --",
    "typeorm": "ts-node ./node_modules/typeorm/cli -f migration.config.ts",
    "typeorm:create": "ts-node ./node_modules/typeorm/cli -f migration.config.ts migration:create",
    "typeorm:generate": "ts-node ./node_modules/typeorm/cli -f migration.config.ts migration:generate -n",
    "typeorm:migration:run": "ts-node ./node_modules/typeorm/cli -f migration.config.ts migration:run"
  },
